# Set defaults.
defaults
# Enable or disable TLS/SSL encryption.
tls off
tls_starttls off
# Setup WP account's settings.
account postfix
host postfix
port 25
auth login
user postfix@jcubic.pl
password postfixpass
from blog@jcubic.pl

logfile /var/log/msmtp/msmtp.log

account default: postfix
